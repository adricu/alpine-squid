sudo apt-get install linux-image-extra-$(uname -r) linux-image-extra-virtual
sudo apt-key adv --keyserver hkp://ha.pool.sks-keyservers.net:80 --recv-keys 58118E89F3A912897C070ADBF76221572C52609D
echo "deb https://apt.dockerproject.org/repo ubuntu-$(lsb_release -c | cut -f2) main" | sudo tee /etc/apt/sources.list.d/docker.list
sudo apt-get update
sudo apt-get install docker-engine
sudo usermod -aG docker $(whoami)

-------

cd squid-docker/
docker build -t adrian/squid:0.1 .
docker run -d -p 3128:3128 --name test adrian/squid:0.1
docker logs test